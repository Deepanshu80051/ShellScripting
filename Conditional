//   CODE
  #!/bin/bash

read -p "enter you name " name
if [[ $name == "baba" ]]; then
        echo "hee"
else
        echo "new"
fi


echo "enter your age"
read age
if [[ $age == 18 ]]; then
        echo "adult"
else
        echo "not"
fi




echo "enter a"
read a

if (( $a % 2 == 0 )); then
        echo "even"
else
        echo "odd"
fi


//command
